+	} ctrl_ui_info;
+
+	/* This Struct is valid only if ctrl_ui_type = 0x03 */
+	struct {
+		uint8_t num_menu_elem;
+		char **menu;
+	} ctrl_menu_info;
+} ISP_CTRL_UI_INFO;
+
+typedef struct _isp_ctrl_info_std {
