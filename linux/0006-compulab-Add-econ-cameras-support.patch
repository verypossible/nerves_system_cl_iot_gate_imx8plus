+static int ar0234_enum_mbus_code(struct v4l2_subdev *sd,struct v4l2_subdev_state *state,struct v4l2_subdev_mbus_code_enum *code)
+{
+	if (code->pad || code->index >= AR0234_MAX_FORMAT_SUPPORTED)
