+static void toggle_gpio(unsigned int gpio, int val)
+{
+	if (gpio_cansleep(gpio)){
+		gpio_direction_output(gpio,val);
+		gpio_set_value_cansleep(gpio, val);
+	} else{
+		gpio_direction_output(gpio,val);
+		gpio_set_value(gpio, val);
+	}
+}
