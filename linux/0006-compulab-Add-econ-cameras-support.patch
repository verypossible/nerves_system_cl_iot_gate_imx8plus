+static int ar0521_g_parm(struct v4l2_subdev *sd, struct v4l2_streamparm *param)
+{
+	int mode = ar0521_data.streamcap.capturemode;
+
+	param->parm.capture.capability |= V4L2_CAP_TIMEPERFRAME;
